{
  "name": "flowaudit-platform",
  "version": "0.1.0",
  "private": true,
  "description": "FlowAudit - Modulare Prüfbehörden-Management-Plattform",
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,vue,md,json}\""
  },
  "devDependencies": {
    "turbo": "^2.3.4",
    "typescript": "^5.7.2",
    "prettier": "^3.4.2"
  }
}
